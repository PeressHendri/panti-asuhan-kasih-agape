{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://your-vercel-domain.vercel.app",
        "LOG_CHANNEL": "stack",
        "CACHE_DRIVER": "file",
        "SESSION_DRIVER": "file",
        "QUEUE_CONNECTION": "sync"
    },
    "build": {
        "env": {
            "APP_ENV": "production",
            "APP_DEBUG": "false"
        }
    },
    "outputDirectory": "public"
} 