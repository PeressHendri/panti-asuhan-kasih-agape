{
    "version": 2,
    "framework": null,
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ],
    "outputDirectory": "public",
    "buildCommand": "npm run build"
} 